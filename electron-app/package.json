{
  "name": "electron-app",
  "version": "0.2.0",
  "main": "dist/main.js",
  "private": true,
  "scripts": {
    "start": "tsc && electron .",
    "build": "webpack -p",
    "package": "electron-builder build --dir --publish never",
    "package:pl": "electron-builder build --config electron-builder.pl.yml --dir --publish never",
    "dist": "electron-builder build",
    "dist:pl": "electron-builder build --config electron-builder.pl.yml"
  },
  "devDependencies": {
    "@types/node": "^12.7.5",
    "electron": "^6.0.10",
    "electron-builder": "^21.2.0",
    "ts-loader": "^6.1.1",
    "typescript": "^3.6.3",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9"
  },
  "workspaces": {
    "nohoist": [
      "electron",
      "electron/**"
    ]
  },
  "dependencies": {
    "i18next": "^17.0.16"
  }
}
